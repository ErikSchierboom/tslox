{
  "name": "tslox",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && npm run run",
    "run": "node dist/main.js",
    "watch": "swc ./src --out-dir dist --source-maps --watch",
    "build": "swc ./src --out-dir dist --source-maps",
    "lint": "eslint && prettier --check .",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "ci": "npm run lint && npm run check && npm test",
    "generate-ast": "swc bin/generate-ast.ts --out-file=dist/generate-ast.js && node dist/generate-ast.js && prettier --write src/Expr.ts src/Stmt.ts"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
